
    <!-- SITE CONTENT -->
    <div class="wrapper">
        <section class="titlebar">
            <h1 class="page-title"><span>Two-factor </span>Authentication</h1>
            <div id="particles-js"></div>
        </section>
        
        <hr class="col-md-6 bottom_60">

        <div class="cont">
            <section class="contact col-md-8 offset-md-2 top_90">
                <div class="contact-info text-center">
                    <p>#35/5, Eldams Road, Alwarpet, Chennai, Tamil Nadu 600018.</p> 
                    <a href="mailto:hello@promo.works">hello@promo.works</a> 
                    <p>+91 94 88 88 99 89</p>
                </div>
                <div class="contact-form top_90">
                           {{#if success_msg}}
            <div class="alert alert-success text-center">{{success_msg}}</div>
          {{/if}}

          {{#if error_msg}}
            <div class="alert alert-danger text-center">{{error_msg}}</div>
          {{/if}}

          {{#if error}}
            <div class="alert alert-danger text-center">{{error}}</div>
          {{/if}}
                    <form class="row" action ="/two-factor/{{user.username}}" method="post" >
                        <div class="col-md-12">
                            <input class="inp" type="text" placeholder="Enter the OTP" name="otp" id="otp" autocomplete="off">
                            <input class="inp" type="hidden" value="{{randomOtp}}" name="Dupkey" id="Dupkey" autocomplete="off">
                            <input class="inp" type="hidden" value="{{mobile}}" name="mobile" id="mobile" autocomplete="off">
                             <div class="col-md-12 text-center">
                                        <a href="" class="text-center" >Didn't receive Code ?</a>
                                     </div>
                            <br/>
                        <div class="col-md-12 text-center">
                            <input type="submit" value="Here We Go" class="site-btn2">
                        </div>
                        </div>
                    </form>
                 
                </div>



            </section>

        </div> <!-- cont end -->
    </div> <!-- wrapper end -->


