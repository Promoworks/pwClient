
<header>
    <a href="/home/home/{{user.username}}"><img src="/images/logo.jpg" alt="" class="img-responsive" width="50px" height="50px"></a>
        <div class="nav-icon">
            <span></span>
            <span></span>
            <span></span>
        </div>
    </header>

<div class="wrapper">

            <section class="titlebar">
            <h1 class="page-title"><span>Ongoing </span>Miscellaneous</h1>
            <div id="particles-js"></div>
        </section>

    
        
    
            {{#each Miscellaneous}}
            {{#if Ongoing}}
    
    
            <div class="cont bg-space">
            <section class="portfolio-single type-2 top_90">

                <div class="portfolio-images top_60 row">
       
                    
                    
                    
                    
                    
                                                                                {{#if pdf}}
                     {{#if Enable}}
                        <a href="/progress/OngoingMiscellaneousLargeView/{{this._id}}/{{this.toWhom}}" class="col-md-12 news-list bottom_60">
                            <article class="news-box row">
                                <figure class="col-md-6">
                                    <iframe src = "{{this.Miscellaneous_final_path}}" width="100%" height="700px" allowfullscreen >
                        
                            <a href="{{this.Miscellaneous_final_path}}" class="text-center">Click here to download the PDF file.</a>
                        
                        </iframe>
                                </figure>
                                <div class="col-md-10 left-list">
                                    <small>{{this.brandtype}}</small>
                                    <h4 class="title">{{this._id}}</h4>
                                    <p>Click Here to View Enlarge ...</p>
                                    <div class="information">{{this.owner}}, &nbsp; &nbsp;<span>{{this.Miscellaneous_date}}</span>&nbsp; &nbsp;<span>{{this.Miscellaneous_time}}</span></div>
                                </div>
                            </article>
                        </a>
                        {{/if}} 
                        {{/if}} 
                    
                    
                    
                    
                                        
                    
                                                                                {{#if videos}}
                     {{#if Enable}}
                        <a href="/progress/OngoingMiscellaneousLargeView/{{this._id}}/{{this.toWhom}}" class="col-md-12 news-list bottom_60">
                            <article class="news-box row">
                                <figure class="col-md-10">
                <video src="{{this.Miscellaneous_final_path}}" class="js-player" controls data-plyr-config='{ "title": "This is an example video", "volume": 1, "debug": true }'></video>
                                </figure>
                                <div class="col-md-10 left-list">
                                    <small>{{this.brandtype}}</small>
                                    <h4 class="title">{{this._id}}</h4>
                                    <p>Click Here to View Enlarge ...</p>
                                    <div class="information">{{this.owner}}, &nbsp; &nbsp;<span>{{this.Miscellaneous_date}}</span>&nbsp; &nbsp;<span>{{this.Miscellaneous_time}}</span></div>
                                </div>
                            </article>
                        </a>
                        {{/if}} 
                        {{/if}} 
                    
                    
                    
                    
                    
                                        
                    
                                        
                    
                                                                                {{#if Image}}
                     {{#if Enable}}
                        <a href="/progress/OngoingMiscellaneousLargeView/{{this._id}}/{{this.toWhom}}" class="col-md-12 news-list bottom_60">
                            <article class="news-box row">
                                <figure class="col-md-6">
                                    <img src="https://promo.works{{this.Miscellaneous_final_path}}" alt="" id="image" name="image" class="img-responsive">
                                </figure>
                                <div class="col-md-10 left-list">
                                    <small>{{this.brandtype}}</small>
                                    <h4 class="title">{{this._id}}</h4>
                                    <p>Click Here to View Enlarge ...</p>
                                    <div class="information">{{this.owner}}, &nbsp; &nbsp;<span>{{this.Miscellaneous_date}}</span>&nbsp; &nbsp;<span>{{this.Miscellaneous_time}}</span></div>
                                </div>
                            </article>
                        </a>
                        {{/if}} 
                        {{/if}} 
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                </div>
            </section>
                        </div>
    
                
                        {{/if}}
                    {{/each}}
    
                         
      
                    <div class="text-center">
                        
                                <a href="/progress/OngoingGiveMeAComment-Miscellaneous/{{user._id}}/{{user.username}}" class="text-center">Click here Leave a Reply</a>
                    </div>

    

    
    

    
    
