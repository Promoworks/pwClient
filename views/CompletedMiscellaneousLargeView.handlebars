<header>
    <a href="/home/home/{{user.username}}"><img src="/images/logo.jpg" alt="" class="img-responsive" width="50px" height="50px"></a>
        <div class="nav-icon">
            <span></span>
            <span></span>
            <span></span>
        </div>
    </header>

<div class="wrapper">
        <section class="titlebar">
            <h1 class="page-title"><span>Miscellaneous </span>Ads</h1>
            <div id="particles-js"></div>
        </section>
        <div class="cont">
            <section class="portfolio-single type-2 top_90">
                <div class="portfolio-images top_60 row">
  {{#each CompletedMiscellaneous}}
                    {{#if Completed }}
{{#if videos }}
                <figure class="col-md-12 bottom_30">
                                             <video src="{{this.Miscellaneous_final_path}}" class="js-player" controls ></video>
                    </figure>

                           
                                                       
                                                                        <div class="col-md-10 left-list">
                                    <small>{{this.brandtype}}</small>
                                    <h4 class="title">{{this._id}}</h4>
                                    <div class="information">Uploaded @ <span>{{this.Miscellaneous_date}}</span>&nbsp; &nbsp;<span>{{this.miscellaneous_time}}</span></div>
                                </div>
                    
                    
        <a class="btn" href="{{this.Miscellaneous_final_path}}" download style="width:100%;background:#1bafff;color: #fff;padding: 12px 12px;" target="_blank" rel="noopener"><i class="fa fa-download"></i> Download</a>                 

                    <div class="leave-reply top_90 container"></div>
                    {{/if}}
                    {{/if}}
                    {{/each}}
                    
                    
  {{#each CompletedMiscellaneous}}
                    {{#if Completed }}
{{#if pdf }}
                <figure class="col-md-12 bottom_30">
                                             <iframe src = "{{this.Miscellaneous_final_path}}" width="100%" height="700px" allowfullscreen ></iframe>
                    </figure>

                           
                                                       
                                                                        <div class="col-md-10 left-list">
                                    <small>{{this.brandtype}}</small>
                                    <h4 class="title">{{this._id}}</h4>
                                    <div class="information">Uploaded @ <span>{{this.Miscellaneous_date}}</span>&nbsp; &nbsp;<span>{{this.miscellaneous_time}}</span></div>
                                </div>
                    
                    
        <a class="btn" href="{{this.Miscellaneous_final_path}}" download style="width:100%;background:#1bafff;color: #fff;padding: 12px 12px;" target="_blank" rel="noopener"><i class="fa fa-download"></i> Download</a>                 

                    <div class="leave-reply top_90 container"></div>
                    {{/if}}
                    {{/if}}
                    {{/each}}

                    
                    
                                      
  {{#each CompletedMiscellaneous}}
                    {{#if Completed }}
{{#if Image }}
                <figure class="col-md-12 bottom_30">
                                            <img src="{{this.Miscellaneous_final_path}}" alt="{{user.username}}" style="width:500px;height:500px;">
                    </figure>

                           
                                                       
                                                                        <div class="col-md-10 left-list">
                                    <small>{{this.brandtype}}</small>
