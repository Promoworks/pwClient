<div class="wrapper">
               <section class="titlebar">
            <h1 class="page-title"><span>Completed Brochure </span>Designs</h1>
            <div id="particles-js"></div>
        </section>

        <div class="cont">
            <section class="portfolio-single type-2 top_90">

                <div class="portfolio-images top_60 row">
  {{#each CompletedBrochure}}
                        {{#if Completed}}

                    <figure class="col-md-12 bottom_30 hidden-xs">
                        <object data="{{this.brochure_final_path}}" type="application/pdf" width="1400" height="1400" style="height:-webkit-fill-available;width:-webkit-fill-available;"></object>

                    </figure>
   
                    
                                        
                                <div class="col-md-10 left-list">
                                    <small>{{this.Adtype}}</small>
                                    <h4 class="title">{{this._id}}</h4>
                                    <p>{{this.brochure_file_name}}</p>
                                    <div class="information">Uploaded @ <span>{{this.brochure_date}}</span>&nbsp; &nbsp;<span>{{this.brochure_time}}</span></div>
                                </div>
                    
                    
                           
         <a class="btn" href="{{this.brochure_final_path}}" download style="width:100%;background:#1bafff;color: #fff;padding: 12px 12px;" target="_blank" rel="noopener"><i class="fa fa-download"></i> Download</a>



                    
                    <div class="leave-reply top_90 container"></div>
                    {{/if}}
                    {{/each}}

                  
                </div>
            </section>

        </div> <!-- cont end -->
    
    

        
        
    
    

    
    
